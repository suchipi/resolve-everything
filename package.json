    "first-base": "^1.4.0",
    "path-less-traveled": "^2.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  },
  "prettier": {},
  "dependencies": {
    "clefairy": "^1.2.0",
    "debug": "^4.3.4",
    "equivalent-exchange": "^1.10.0",
    "kleur": "^4.1.5",
    "parallel-park": "^0.2.0",
    "pretty-print-ast": "^1.0.1",
    "resolve": "^1.22.8"
  },
  "repository": {
